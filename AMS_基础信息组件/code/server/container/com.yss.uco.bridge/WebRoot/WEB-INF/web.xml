<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<filter>
		<filter-name>monitoring</filter-name>
		<filter-class>net.bull.javamelody.MonitoringFilter</filter-class>
		<init-param>
			<param-name>log</param-name>
			<param-value>false</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>monitoring</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>net.bull.javamelody.SessionListener</listener-class>
	</listener>
	<!-- 自定义的容器监听器 -->
	<listener>
		<listener-class>listener.ContainerListener</listener-class>
	</listener>


	<servlet>
		<servlet-name>equinoxbridgeservlet</servlet-name>
		<!-- <servlet-class>org.eclipse.equinox.servletbridge.BridgeServlet</servlet-class> -->
		<servlet-class>servlet.AMSServletBridge</servlet-class>

		<init-param>
			<param-name>commandline</param-name>
			<param-value>-console</param-value>
		</init-param>
		<init-param>
			<param-name>enableFrameworkControls</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>extendedFrameworkExports</param-name>
			<param-value></param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>amsDownloadServlet</servlet-name>
		<servlet-class>servlet.DownloadServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>equinoxbridgeservlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>equinoxbridgeservlet</servlet-name>
		<url-pattern>*.jsp</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>amsDownloadServlet</servlet-name>
		<url-pattern>/file/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>amsDownloadServlet</servlet-name>
		<url-pattern>/yssdataintegration/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>amsDownloadServlet</servlet-name>
		<url-pattern>/unifypay/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>amsDownloadServlet</servlet-name>
		<url-pattern>/ysselecreco/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>amsDownloadServlet</servlet-name>
		<url-pattern>/YSSMONITOR/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	  <servlet-name>amsDownloadServlet</servlet-name>
	  <url-pattern>/transfer/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	  <servlet-name>amsDownloadServlet</servlet-name>
	  <url-pattern>/dbupgrade/outfile/*</url-pattern>
	</servlet-mapping>
</web-app>