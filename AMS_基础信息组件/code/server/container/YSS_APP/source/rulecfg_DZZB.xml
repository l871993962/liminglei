<?xml version="1.0" encoding="UTF-8"?>
<source code="DZZB" name="对账指标关联" funcode="dzPubRela" datasource="DB">
	<exp>
		<data id="zb">
			<table-name>T_Z_BI_RELA</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_Z_BI_RELA where C_ZB_CODE in (select * from table(?))</sql>
				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_ZB_CODE</value>
					</cons_column>
				</cons>

			</sqlmapping>			
		</data>	
	</exp>
	
	<!-- 数据导入配置 -->
	<imp>
		<!-- name对应的需要导入的文件名 opertype为重复数据处理机制 cover覆盖原数据 rename重新赋值筛选字段数据-->
		<outfile name="zb">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_ZB_CODE</check_column>
		</outfile>
	</imp>
</source>
