<?xml version="1.0" encoding="UTF-8"?>
<source code="SJQY" name="数据迁移" funcode="GGSJZH" datasource="DB">
	<!-- 数据导出配置 -->
	<exp>
		<data id="expression">
			<table-name>t_d_sqlcfg</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from t_d_sqlcfg where c_info_desc in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>c_info_desc</value>
					</cons_column>
				</cons>

			</sqlmapping>			
		</data>	

	</exp>
	
	<imp>
		<outfile name="expression">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>c_info_desc</check_column>
		</outfile>
	</imp>

</source>
