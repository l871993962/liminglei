<?xml version="1.0" encoding="UTF-8"?>
<source code="BDS" name="表达式" funcode="deexpquy" datasource="DB">
	<!-- 数据导出配置 -->
	<exp>
		<data id="expression">
			<table-name>T_S_DE_EXP</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_DE_EXP where C_EXP_CODE in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_EXP_CODE</value>
					</cons_column>
				</cons>

			</sqlmapping>			
		</data>	

	</exp>
	
	<imp>
		<outfile name="expression">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_EXP_CODE</check_column>
		</outfile>
	</imp>

</source>
