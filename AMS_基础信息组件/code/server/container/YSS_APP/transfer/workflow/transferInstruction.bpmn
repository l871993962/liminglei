        <omgdi:waypoint x="688.0" y="477.0"></omgdi:waypoint>
        <omgdi:waypoint x="688.0" y="186.0"></omgdi:waypoint>
        <omgdi:waypoint x="645.0" y="187.0"></omgdi:waypoint>
        <bpmndi:BPMNLabel>
          <omgdc:Bounds height="14.0" width="24.0" x="437.0" y="457.0"></omgdc:Bounds>
