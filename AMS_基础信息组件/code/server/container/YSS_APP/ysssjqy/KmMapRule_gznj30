--拼接证券内码
update md_km_map_temp a
   SET a.c_sec_code = (select b.c_sec_code  from t_p_sv_sec_base b
                        where substr(b.c_sec_code,0,6) = substr(a.facctcode,length(a.facctcode)-5,6) and b.c_sec_var_code like 'LC_ZQJH%' 
                          AND B.C_MKT_CODE = A.C_MKT_CODE and rownum = 1)                    
  where a.FACCTATTR like '%债权%'  AND a.facctlevel=4;
UPDATE md_km_map_temp a
   SET a.c_Sec_Var_Code =  nvl((select b.c_Sec_Var_Code   from t_p_sv_sec_base b 
        where b.c_sec_code  = a.c_sec_code and b.c_mkt_code = a.c_mkt_code ),'LC_ZQJH')
           where a.FACCTATTR like '%债权%'  ;
update md_km_map_temp a set c_dta_code = ' ', C_DV_ISSUE_MODE = ' ', c_mkt_code='COTC',C_DV_INVEST_CLS='IC_TD'
  where a.FACCTATTR like '%债权%' AND a.facctlevel=4  ;
  update md_km_map_temp a set c_dai_code = 'YSLX_ZQ'
  where a.FACCTATTR like '%应收利息_债权%' AND a.facctlevel=4  ;
UPDATE md_km_map_temp a
   SET a.c_dv_invest_mode =  nvl((select b.c_dv_qut_mod  from t_p_sv_sec_base b 
        where b.c_sec_code  = a.c_sec_code and b.c_mkt_code = a.c_mkt_code and  c_sec_var_code like 'LC_ZQJH%'),a.c_dv_invest_mode)
        where a.FACCTATTR like '%债权%';

