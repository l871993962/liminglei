--补漏
--证券内码
update md_km_map_temp a
   SET a.c_sec_code =  nvl((select max(b.C_SEC_CODE) from t_p_sv_sec_base b 
        where SUBSTR(b.c_sec_code, 0, instr(b.c_sec_code,' ',1,1)-1) = substr(UPPER(a.facctcode),length(a.facctcode)-5,6) ),a.C_SEC_CODE)
                          where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%' or a.FACCTATTR like '%回购%' 
                          or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%')
                          and a.FACCTATTR NOT like '%费%' and a.FACCTATTR NOT like '%清算款%' and a.c_sec_code is null;
--市场代码
update md_km_map_temp a
   SET a.c_mkt_code =  nvl((select max(b.c_mkt_code) from t_p_sv_sec_base b 
        where SUBSTR(b.c_sec_code, 0, instr(b.c_sec_code,' ',1,1)-1) = substr(UPPER(a.facctcode),length(a.facctcode)-5,6) ),a.c_mkt_code)
                          where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%' or a.FACCTATTR like '%回购%'
                           or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%')
                          and a.FACCTATTR NOT like '%费%' and a.FACCTATTR NOT like '%清算款%' and a.c_mkt_code is null;

update md_km_map_temp a SET a.c_dta_code = 'PT'
where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%'
 or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%')
and a.FACCTATTR NOT like '%费%' and a.FACCTATTR NOT like '%清算款%' and a.c_dta_code is null;
update md_km_map_temp a SET a.c_dv_issue_mode = 'SS'
where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%'
 or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%')
and a.FACCTATTR NOT like '%费%' and a.FACCTATTR NOT like '%清算款%' and a.c_dv_issue_mode is null;
update md_km_map_temp a SET a.c_dv_invest_cls = 'IC_TD'
where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%'
 or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%')
and a.FACCTATTR NOT like '%费%' and a.FACCTATTR NOT like '%清算款%' and a.c_dv_invest_cls is null;
                         
--通过基本信息修正品种
UPDATE md_km_map_temp a
   SET a.c_Sec_Var_Code =  nvl((select b.c_Sec_Var_Code   from t_p_sv_sec_base b 
        where b.c_sec_code  = a.c_sec_code and b.c_mkt_code = a.c_mkt_code ),a.c_Sec_Var_Code)
           where (a.FACCTATTR like '%债券%' or a.FACCTATTR like '%基金%' or a.FACCTATTR like '%股票%' or a.FACCTATTR like '%回购%'
            or a.FACCTATTR like '%融资券%' or a.FACCTATTR like '%票据%' or a.FACCTATTR like '%债权%' or a.FACCTATTR like '%保险产品%' or a.FACCTATTR like '%养老金产品%') and a.c_Sec_Var_Code is null;
           
