   set c_sec_code = nvl((select max(c_sec_code) c_sec_code
       from T_P_SV_SEC_BASE b
       where c_sec_var_code like 'CK%'
       and a.facctname = (select max(c.facctname)
     from md_laccount c where c.facctname=a.facctname and c.facctcode in(
    select d.flxkm  from md_yhckyw d where d.flxkm=a.facctcode and c.c_fset = d.c_fset))), 
     a.c_sec_code)
     where a.FACCTATTR like '%应收利息%存款%' AND a.FACCTATTR NOT like '%活期%' AND a.facctclass = '资产类';



