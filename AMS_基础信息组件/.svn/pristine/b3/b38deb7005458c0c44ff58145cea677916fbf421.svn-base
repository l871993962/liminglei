﻿using FAST.Core.Util;
using FAST.Common.Service.Pojo;
using FAST.Common.Service.Pojo.Base;
using FAST.Core.Exceptions;


using FAST.Core.Context;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

using Newtonsoft.Json;
using YssInformation.Support.Statepojo;

namespace YssInformation.Support.Sys.DcCury.Pojo
{
    /// <summary>
    /// 币种信息pojo类
    /// Added by huangsq 20160728 STORY #32244 词汇资源可控制是否显示
    /// </summary>
    public class DcCury : StatePojo
    {
        /// <summary>
        /// 币种代码
        /// </summary>
        private string c_DC_CODE = "";

        /// <summary>
        /// 币种名称
        /// </summary>
        private string c_DC_NAME = "";

        /// <summary>
        ///  币种符号
        /// </summary>
        private string c_DC_SIGN = "";

        /// <summary>
        /// 编号
        /// </summary>
        private int n_ORDER = 0;

        /// <summary>
        /// 属性: 币种代码 
        /// </summary>
        [JsonProperty(PropertyName = "c_DC_CODE")]
        public string C_DC_CODE
        {
            set { c_DC_CODE = value; }

            get { return c_DC_CODE; }
        }

        /// <summary>
        /// 属性: 币种名称 
        /// </summary>
        [JsonProperty(PropertyName = "c_DC_NAME")]
        public string C_DC_NAME
        {
            set { c_DC_NAME = value; }

            get { return c_DC_NAME; }
        }

        /// <summary>
        /// 属性: 币种符号 
        /// </summary>
        [JsonProperty(PropertyName = "c_DC_SIGN")]
        public string C_DC_SIGN
        {
            set { c_DC_SIGN = value; }

            get { return c_DC_SIGN; }
        }

        /// <summary>
        /// 编号
        /// </summary>
        [JsonProperty(PropertyName = "n_ORDER")]
        public int N_ORDER
        {
            set { n_ORDER = value; }

            get { return n_ORDER; }

        }

    }
}




