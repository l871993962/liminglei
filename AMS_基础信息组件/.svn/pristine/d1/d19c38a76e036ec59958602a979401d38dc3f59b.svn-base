<?xml version="1.0" encoding="UTF-8"?>
<source code="BZXX" name="币种信息" funcode="currencyquy" datasource="DB">
	<!-- 数据导出配置 -->
	<exp>
		<!-- filename:数据导出的XML文件名 -->
		<data id="currency">
			<table-name>T_S_DC_CURY</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_DC_CURY where C_DC_CODE in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_DC_CODE</value>
					</cons_column>
				</cons>

			</sqlmapping>			
		</data>	

	</exp>
	
	<!-- 数据导入配置 -->
	<imp>
		<!-- name对应的需要导入的文件名 opertype为重复数据处理机制 cover覆盖原数据 rename重新赋值筛选字段数据-->
		<outfile name="currency">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_DC_CODE</check_column>
		</outfile>
	</imp>

</source>
