update md_km_map_temp set c_sec_var_code = 'GP_GP', C_DV_ISSUE_MODE = 'WSS_WS' WHERE  FACCTATTR like '%投资%' AND (FACCTATTR like '%新股%' or FACCTATTR like '%未上市%') AND facctclass = '资产类'   {and fyear=:year} {and fjjdm=:port}  ;
