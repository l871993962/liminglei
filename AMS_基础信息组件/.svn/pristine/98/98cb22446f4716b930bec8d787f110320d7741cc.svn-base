--建立科目对照表 
insert into md_km_map_temp
  (facctcode,
   facctname,
   facctlevel,
   facctparent,
   facctdetail,
   facctclass,
   facctattr,
   facctattrid,
   fcurcode,
   fbaldc,
   famount,
   fcarryacc,
   fename,
   fby,
   fset,
   fyear,
   fjjdm
   )
select a.facctcode,a.facctname,a.facctlevel,a.facctparent,a.facctdetail,a.facctclass,
         a.facctattr,a.facctattrid,a.fcurcode,a.fbaldc,a.famount,a.fcarryacc,
         a.fename,a.fby,a.c_fset,a.c_year
, c.fjjdm  	
  from md_laccount a
  left join md_cssysjj c
    on c.c_fset = a.c_fset
  
  where  a.facctdetail = 1 {and a.c_year=:year}
  and  not exists (
     select 1 from md_km_map mkmp
     where a.facctcode = mkmp.facctcode
     and a.c_fset = mkmp.fset 
     {and mkmp.fyear=:year}
  ) 
   and exists (select 1 from md_vchkm f where f.c_fset = a.c_fset and {f.c_year=:year} and  f.fkmh =a.facctcode)
    {and c.fjjdm=:port} ;

update md_km_map_temp set c_plan_code =:c_plan_code where  { fyear=:year} {and fjjdm=:port} ;

--C_DV_KM_CLS 科目类别
update md_km_map_temp set c_dv_km_cls  = case when facctclass = '资产类' then 'KC_ZC' when facctclass = '负债类' then 'KC_FZ' 
 when facctclass = '共同类' then 'KC_GT' when facctclass = '权益类' then 'KC_QY' when facctclass = '损益类' then 'KC_SY' else ' ' end
 WHERE (c_dv_km_cls = '' or c_dv_km_cls is null)   {and fyear=:year} {and fjjdm=:port}  ;

--先设默认核算项目
update md_km_map_temp set C_DAI_CODE  = ' ' where  { fyear=:year} {and fjjdm=:port}  ;

--借贷方向C_DV_JD_WAY(JD_J,JD_D)
update md_km_map_temp set C_DV_JD_WAY  = 'JD_J' WHERE fbaldc = 1 and (C_DV_JD_WAY = '' or C_DV_JD_WAY is null)    {and fyear=:year} {and fjjdm=:port}  ;
update md_km_map_temp set C_DV_JD_WAY  = 'JD_D' WHERE fbaldc = -1 and (C_DV_JD_WAY = '' or C_DV_JD_WAY is null)   {and fyear=:year} {and fjjdm=:port}  ;

--更新币种
update md_km_map_temp set c_dc_code = (case when fcurcode = 'RMB' then 'CNY' else fcurcode end) where fcurcode = 'RMB'  ;

--股票投资交易属性为普通
update md_km_map_temp  set  c_dta_code = 'PT' where facctattr like '%股票投资%' and facctclass = '资产类'   {and fyear=:year} {and fjjdm=:port}  ;

--银行存款、备付金、保证金 （非股指期货）
update md_km_map_temp a set C_DAI_CODE  = 'HBZJ',c_ca_code = fjjdm||'_'|| facctcode
  WHERE (FACCTATTR like '银行存款%' or FACCTATTR like '清算备付金%' or FACCTATTR like '结算保证金%' or FACCTATTR like '交易保证金%') AND  FACCTATTR NOT like '%股指期货%' 
  AND facctclass = '资产类' AND facctclass = '资产类'   {and fyear=:year} {and fjjdm=:port}  ;
  
  --银行存款、备付金、保证金 （股指期货，国债期货）
update md_km_map_temp a set C_DAI_CODE  = 'HBZJ',c_ca_code = fjjdm||'_'|| facctcode
  WHERE (FACCTATTR like '银行存款%' or FACCTATTR like '清算备付金%' or FACCTATTR like '结算保证金%' or FACCTATTR like '交易保证金%' or FACCTATTR like '存出保证金%') AND  FACCTATTR like '%期货%' 
  AND facctclass = '资产类' AND facctclass = '资产类'   {and fyear=:year} {and fjjdm=:port}  ;
  
--更新存款类别
update md_km_map_temp a set a.c_dv_acc_type  = (select c_dv_acc_type from T_P_BI_CASH_ACC b where a.c_ca_code = b.c_ca_code) where a.c_dv_acc_type = ''   {and fyear=:year} {and fjjdm=:port}  ; 
update md_km_map_temp a set a.c_dc_code  = (select c_dc_code from T_P_BI_CASH_ACC b where a.c_ca_code = b.c_ca_code) where a.c_dv_acc_type = ''   {and fyear=:year} {and fjjdm=:port}  ; 

