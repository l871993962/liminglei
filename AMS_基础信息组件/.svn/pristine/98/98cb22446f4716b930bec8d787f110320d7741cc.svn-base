UPDATE md_km_map_temp a SET a.c_mkt_code = 'XCFE',a.C_SEC_VAR_CODE='ZQ_QT', a.C_DV_ISSUE_MODE='' 
 where a.FACCTATTR like '%其他债券'  AND a.facctclass = '资产类' and facctname like '%期%年%' and (c_km_Code is null or length(c_km_Code)<2) ;


--根据债券品种信息更新债券代码
--对于不同市场的相同债券代码的债券后面处理,
update md_km_map_temp a
   SET a.c_sec_code = (select fzqdm||' '||case when fszsh = 'H' then 'SH' when fszsh = 'S' then 'SZ' when fszsh = 'Y' then 'CY' else ' ' end as zqdm
                        from (select fzqdm, max(fszsh) as fszsh from md_cszqxx where fjjdm=' ' group by fzqdm having COUNT(fzqdm)=1
                             ) b
                        where b.fzqdm = substr(a.facctcode,length(a.facctcode)-5,6) )
                         
                          where (a.FACCTATTR like '%债券投资%' or a.FACCTATTR like '%溢折价%') AND a.facctclass = '资产类' and c_km_Code is null {and fjjdm=:port} {and fyear=:year};

--根据债券品种信息更新债券代码
--对于不同市场的相同债券代码的债券处理,
update md_km_map_temp a
   SET a.c_sec_code = (select fzqdm||' '||case when fszsh = 'H' then 'SH' when fszsh = 'S' then 'SZ' when fszsh = 'Y' then 'CY' else ' ' end as zqdm
                        from md_cszqxx b
                        where b.fzqdm = substr(a.facctcode,length(a.facctcode)-5,6) and 
                         case when a.FACCTATTR like '%银行间%' then 'Y' when a.FACCTATTR like '%深圳%' then 'S' else 'H' end = b.fszsh)
                         
                          where (a.FACCTATTR like '%债券投资%' or a.FACCTATTR like '%溢折价%') AND a.facctclass = '资产类' 
                          and (c_sec_code=' ' or c_sec_code is null) and c_km_Code is null   {and fjjdm=:port} {and fyear=:year};
 

--应收利息 根据债券品种信息更新债券代码
--对于不同市场的相同债券代码的债券后面处理,
update md_km_map_temp a
   SET a.c_sec_code = (select fzqdm||' '||case when fszsh = 'H' then 'SH' when fszsh = 'S' then 'SZ' when fszsh = 'Y' then 'CY' else ' ' end as zqdm
                        from (select fzqdm, max(fszsh) as fszsh from md_cszqxx where fjjdm=' ' group by fzqdm having COUNT(fzqdm)=1
                             ) b
                        where b.fzqdm = substr(a.facctcode,length(a.facctcode)-5,6) )
                         
                          where (  a.FACCTATTR like '%应收利息_%债%'  or a.FACCTATTR like '%应收利息_%融资券%'  or FACCTATTR  LIKE '应收利息_%票据%' or FACCTATTR  LIKE '应收利息_%资产%证券化%')
                       AND a.facctclass = '资产类' and c_km_Code is null {and fjjdm=:port} {and fyear=:year};

----应收利息 根据债券品种信息更新债券代码
----对于不同市场的相同债券代码的债券处理,
update md_km_map_temp a
   SET a.c_sec_code = (select fzqdm||' '||case when fszsh = 'H' then 'SH' when fszsh = 'S' then 'SZ' when fszsh = 'Y' then 'CY' else ' ' end as zqdm
                        from md_cszqxx b
                        where b.fzqdm = substr(a.facctcode,length(a.facctcode)-5,6) and 
                         case when a.FACCTATTR like '%银行间%' then 'Y' when a.FACCTATTR like '%深圳%' then 'S' else 'H' end = b.fszsh)
                         
                          where ( a.FACCTATTR like '%应收利息_%债%' or a.FACCTATTR like '%应收利息_%融资券%' or FACCTATTR  LIKE '应收利息_%票据%' or FACCTATTR  LIKE '应收利息_%资产%证券%')
                             AND a.facctclass = '资产类' 
                          and (c_sec_code=' ' or c_sec_code is null) and c_km_Code is null    {and fjjdm=:port} {and fyear=:year};
 

