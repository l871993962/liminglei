<?xml version="1.0" encoding="UTF-8"?>
<source code="CUSTOM" name="功能管理" funcode="customFun" datasource="DB">
	<exp>
		<data id="CUSTOM">
			<table-name>T_S_FUN_CUSTOM</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_FUN_CUSTOM where C_IDEN in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>id</value>
					</cons_column>
				</cons>
			</sqlmapping>			
		</data>	
	</exp>
	
	<!-- 数据导入配置 -->
	<imp>
		<!-- name对应的需要导入的文件名 opertype为重复数据处理机制 cover覆盖原数据 rename重新赋值筛选字段数据-->
		<outfile name="CUSTOM">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_FUN_CODE</check_column>
			<check_column>C_FUN_CODE_P</check_column>
			<check_column>C_DV_APP_CLS</check_column>
			<check_column>N_DEFAULT</check_column>
		</outfile>
	</imp>

</source>
