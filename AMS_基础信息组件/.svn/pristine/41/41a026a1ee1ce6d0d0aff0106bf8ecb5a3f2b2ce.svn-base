      <fileNamePattern>${logHome}/MQ/%d{yyyy-MM,aux}/broker_%d{yyyy-MM-dd}_%i.log</fileNamePattern>  
      <minIndex>${log.minIndex}</minIndex>  
      <maxIndex>${log.maxIndex}</maxIndex>  
