<?xml version="1.0" encoding="UTF-8"?>
<source code="JZZBFA" name="净值指标方案" funcode="indexdiquy" datasource="DB">
	<exp>
		<data id="index">
			<table-name>T_S_INDEX</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_INDEX where C_INDEX_CODE in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_INDEX_CODE</value>
					</cons_column>					
				</cons>

			</sqlmapping>			
		</data>	
	</exp>
	
	<imp>
		<outfile name="index">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_INDEX_CODE</check_column>		
		</outfile>
	</imp>

</source>
