package com.yss.sys.ws;

import javax.activation.DataHandler;

public class FileTransPojo {

	private DataHandler fileDataHandler = null;

	private String fileName = "";
	private int action;
	private String fileType;
	private String errInfo = "";

	private String result = "";

	private String toUser = "";

	private String toApp = "";

	private String fromUser = "";

	private String fromApp = "";

	private String pkgSecretCode = "";

	private String commType = "";

	private int priority = 0;

	private String pkgId = "";

	private String busiKey = "";
	private String busType;

	public String getFileName() {
		return this.fileName;
	}

	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	public int getAction() {
		return this.action;
	}

	public void setAction(int action) {
		this.action = action;
	}

	public String getFileType() {
		return this.fileType;
	}

	public void setFileType(String fileType) {
		this.fileType = fileType;
	}

	public DataHandler getFileDataHandler() {
		return this.fileDataHandler;
	}

	public void setFileDataHandler(DataHandler fileDataHandler) {
		this.fileDataHandler = fileDataHandler;
	}

	public String getErrInfo() {
		return this.errInfo;
	}

	public void setErrInfo(String errInfo) {
		this.errInfo = errInfo;
	}

	public String getResult() {
		return this.result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public String getToUser() {
		return this.toUser;
	}

	public void setToUser(String toUser) {
		this.toUser = toUser;
	}

	public String getToApp() {
		return this.toApp;
	}

	public void setToApp(String toApp) {
		this.toApp = toApp;
	}

	public String getFromUser() {
		return this.fromUser;
	}

	public void setFromUser(String fromUser) {
		this.fromUser = fromUser;
	}

	public String getFromApp() {
		return this.fromApp;
	}

	public void setFromApp(String fromApp) {
		this.fromApp = fromApp;
	}

	public String getPkgPassword() {
		return this.pkgSecretCode;
	}

	public void setPkgPassword(String pkgPassword) {
		this.pkgSecretCode = pkgPassword;
	}

	public String getCommType() {
		return this.commType;
	}

	public void setCommType(String commType) {
		this.commType = commType;
	}

	public int getPriority() {
		return this.priority;
	}

	public void setPriority(int priority) {
		this.priority = priority;
	}

	public String getPkgId() {
		return this.pkgId;
	}

	public void setPkgId(String pkgId) {
		this.pkgId = pkgId;
	}

	public String getBusiKey() {
		return this.busiKey;
	}

	public void setBusiKey(String busiKey) {
		this.busiKey = busiKey;
	}

	public String getBusType() {
		return this.busType;
	}

	public void setBusType(String busType) {
		this.busType = busType;
	}
