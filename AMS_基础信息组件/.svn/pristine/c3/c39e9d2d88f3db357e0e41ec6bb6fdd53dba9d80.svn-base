<?xml version="1.0" encoding="UTF-8"?>
<source code="CHZD" name="词汇字典" funcode="pubvocabularyquy" datasource="DB">
	<!-- 数据导出配置 -->
	<exp>
		<data id="vocabulary">
			<table-name>T_S_DV_VOC</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_DV_VOC where C_DV_CODE in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_DV_CODE</value>
					</cons_column>
				</cons>

			</sqlmapping>			
		</data>	

	</exp>
	
	<imp>
		<outfile name="vocabulary">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_DV_CODE</check_column>
		</outfile>
	</imp>

</source>
