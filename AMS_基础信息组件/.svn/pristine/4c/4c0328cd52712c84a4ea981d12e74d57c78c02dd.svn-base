<?xml version="1.0" encoding="UTF-8"?>
<source code="HSCS" name="核算参数" funcode="dspparaquy" datasource="DB">
	<!-- 数据导出配置 -->
	<exp>
		<data id="para">
			<table-name>T_S_DSP_PARA</table-name>
			<!-- 数据查询SQL -->
			<sqlmapping>
				<!--查询表-->
				<sql>select * from T_S_DSP_PARA where C_DSP_CODE in (select * from table(?))</sql>

				<cons>
					<cons_column ref-data="" ref-column="">
						<value>C_DSP_CODE</value>
					</cons_column>					
				</cons>

			</sqlmapping>			
		</data>	
	</exp>
	
	<imp>
		<outfile name="para">
			<!-- 在保存数据前需要检测数据是否已经存在 column为筛选条件 -->
			<check_column>C_DSP_CODE</check_column>		
		</outfile>
	</imp>

</source>
