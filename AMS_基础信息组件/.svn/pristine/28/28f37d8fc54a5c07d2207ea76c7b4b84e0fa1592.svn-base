using FAST.Common.Service.Pojo.Base;
using Newtonsoft.Json;
using FAST.Common.Service.Datastructure;

////namespace FAST.Platform.Dict.Pojo
////namespace YssInformation.Sys.Dictionary.Pojo
namespace YssInformation.Support.Sys.Dictionary.Pojo
{
    /// <summary>
    /// 销售/交易方式pojo类
    /// </summary>
    [NodeDesc(ParentNode = "C_BUSI_TYPE", TreeNode = "C_DT_CODE")]
    public class DttdMode : BasePojo
    {
        /// <summary>
        /// 交易方式代码
        /// </summary>
        private string c_DT_CODE;

        /// <summary>
        /// 交易方式名称
        /// </summary>
        private string c_DT_NAME;

        /// <summary>
        /// 业务类型
        /// </summary>
        private string c_BUSI_TYPE;

        /// <summary>
        /// 资金方向（1－流入, -1-流出，0－无)
        /// </summary>
        private int n_FUND_WAY;

        /// <summary>
        /// 资本方向（1－流入, -1-流出，0－无)
        /// </summary>
        private int n_CAPI_WAY;

        /// <summary>
        /// 编号
        /// </summary>
        private int n_ORDER;

        /// <summary>
        /// 属性: C_DT_CODE 
        /// </summary>
        [JsonProperty(PropertyName = "c_DT_CODE")]
        public string C_DT_CODE
        {
            set { c_DT_CODE = value; }

            get { return c_DT_CODE; }
        }


        /// <summary>
        /// 属性: C_DT_NAME 
        /// </summary>
        [JsonProperty(PropertyName = "c_DT_NAME")]
        public string C_DT_NAME
        {
            set { c_DT_NAME = value; }

            get { return c_DT_NAME; }
        }

        /// <summary>
        /// 属性: C_BUSI_TYPE 
        /// </summary>
        [JsonProperty(PropertyName = "c_BUSI_TYPE")]
        public string C_BUSI_TYPE
        {
            set { c_BUSI_TYPE = value; }

            get { return c_BUSI_TYPE; }
        }

        /// <summary>
        /// 属性: N_FUND_WAY 
        /// </summary>
        [JsonProperty(PropertyName = "n_FUND_WAY")]
        public int N_FUND_WAY
        {
            set { n_FUND_WAY = value; }

            get { return n_FUND_WAY; }
        }

        /// <summary>
        /// 属性: N_CAPI_WAY 
        /// </summary>
        [JsonProperty(PropertyName = "n_CAPI_WAY")]
        public int N_CAPI_WAY
        {
            set { n_CAPI_WAY = value; }

            get { return n_CAPI_WAY; }
        }

        /// <summary>
        /// 属性: N_ORDER 
        /// </summary>
        [JsonProperty(PropertyName = "n_ORDER")]
        public int N_ORDER
        {
            set { n_ORDER = value; }

            get { return n_ORDER; }
        }

    }
}
