--建立科目对照表 
insert into md_km_map_temp
  (facctcode,
   facctname,
   facctlevel,
   facctparent,
   facctdetail,
   facctclass,
   facctattr,
   facctattrid,
   fcurcode,
   fbaldc,
   famount,
   fcarryacc,
   fename,
   fby,
   fset,
   fyear,
   fjjdm
   )
select a.facctcode,a.facctname,a.facctlevel,a.facctparent,a.facctdetail,a.facctclass,
         a.facctattr,a.facctattrid,a.fcurcode,a.fbaldc,a.famount,a.fcarryacc,
         a.fename,a.fby,a.c_fset,a.c_year,
  C.ZTXX
  from md_laccount a
  left join ZTXX_CPDM c
     on SUBSTR(C.ZTXX,-3) = a.c_fset
  where  a.facctdetail = 1 {and a.c_year=:year}
  and  not exists (
     select 1 from md_km_map mkmp
     where a.facctcode = mkmp.facctcode
     and a.c_fset = mkmp.fset 
     {and mkmp.fyear=:year}
  ) 
   and exists 
   (select 1 from md_fcwvch f where f.c_fset = a.c_fset and {f.c_year=:year} and  f.fkmh =a.facctcode  
    UNION 
   select 1 from md_lbalance f where f.fendbal <>0 and  f.fisdetail = 1 AND F.FMONTH = 0 and f.c_fset = a.c_fset and {f.c_year=:year} and  f.facctcode =a.facctcode)
   {and C.ZTXX =:port};

