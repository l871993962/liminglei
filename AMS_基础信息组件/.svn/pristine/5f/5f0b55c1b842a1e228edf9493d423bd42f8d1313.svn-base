 when facctclass = '共同类' then 'KC_GT' when facctclass = '权益类' then 'KC_QY' when facctclass = '损益类' then 'KC_SY' else ' ' end
 WHERE (c_dv_km_cls = '' or c_dv_km_cls is null)     ;

