@CHARSET "UTF-8";

.cover_div {
	display: none;
	position: absolute;
	top: 0%;
	left: 0%;
	width: 100%;
	height: 100%;
	background-color: black;
	z-index: 1001;
	-moz-opacity: 0.7;
	opacity: .70;
	filter: alpha(opacity = 70);
}

.message_div {
	display: none;
	position: absolute;
	top: 25%;
	left: 22%;
	width: 53%;
	height: 49%;
	padding: 8px;
	border: 8px solid #E8E9F7;
	background-color: white;
	z-index: 1002;
	overflow: auto;
}

.index_head{
	background: url('img/bg-header.png') repeat-x;
	border-style: solid;
	/*border-color: #d9eef7;*/
	border-color: #4798B3;
	border-width: 1px;
	height: 50px;
	width: 1400px;
	margin-bottom: 1px;
}

.index_main{
	padding-top:0px;
	border-style: solid;
	border-color: #4798B3;
	border-width: 1px;
	height: 700px;
	width: 1400px;
	/*overflow-x:auto;*/
	overflow-y:auto;
	SCROLLBAR-FACE-COLOR: #d9eef7; 
	SCROLLBAR-SHADOW-COLOR: #d9eef7; 
	SCROLLBAR-HIGHLIGHT-COLOR: #d9eef7; 
	SCROLLBAR-3DLIGHT-COLOR: #d9eef7; 
