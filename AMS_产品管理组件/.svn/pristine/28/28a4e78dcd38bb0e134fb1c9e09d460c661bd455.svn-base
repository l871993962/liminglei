	/**
	 * 根据资产类型代码获取对应的对象（对象里存的是对应资产类型的所有组合）
	 */
	@Override
	public PortCacheData getCacheTrCodeDataMapByKey(String key){
		PortCacheData portCacheData = new PortCacheData();
		portCache.refreshAssetTreeByTrCode(key);
		if (portCache.getPortCacheMap_ZCLX() != null ) {
		    portCacheData.setPortData(portCache.getPortCacheMap_ZCLX().get(key));
		} else {
		    portCacheData.setPortData(new HashMap<String, Port>());
		}
		
		return portCacheData;
	}
	
	
	/**
	 * 获取所有的资产类型代码
	 */
	@Override
	public List<String> getCacheTrCodeDataKey(){
		List<String> result = new ArrayList<String>();
		if(portCache != null){
			if(portCache.getPortCacheMap_ZCLX().isEmpty()){
				portCache.reloadData();
			}
			result = new ArrayList<String>(portCache.getPortCacheMap_ZCLX().keySet());
		}
		return result;
		
	}

