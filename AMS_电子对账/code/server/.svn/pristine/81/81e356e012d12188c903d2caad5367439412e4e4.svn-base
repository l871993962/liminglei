			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<!-- 自定义打包插件 -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<!-- 打出来的jar名称不含Assembly Descriptor文件id -->
					<appendAssemblyId>false</appendAssemblyId>
					<!-- 指定Assembly Descriptor -->
					<descriptors>
						<descriptor>assemblies.xml</descriptor>
					</descriptors>
					<!-- 输出构建的jar到哪个目录 -->
					<outputDirectory>target</outputDirectory>
				</configuration>
				<!-- 绑定到package生命周期 -->
				<executions>
					<execution>
						<id>make-assembly</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
