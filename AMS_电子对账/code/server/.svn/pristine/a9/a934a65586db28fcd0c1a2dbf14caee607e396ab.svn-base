package com.yss.uco.elecreco.support.restservice.pojo;

import com.fasterxml.jackson.annotation.JsonIgnore;
import com.fasterxml.jackson.annotation.JsonProperty;
import com.yss.framework.api.restful.pojo.CommonRequestWrapper;

public class RestfulRequestWrapper extends CommonRequestWrapper {

	@JsonProperty
	private String code;
	
	@JsonProperty
	private String flag;
	
	@JsonProperty
	private String session;

	@JsonProperty
	private String timestamp;

	@JsonProperty
	private String from;

	@JsonProperty
	private String to;

	@JsonProperty
	private int total;

	@JsonProperty
	private Object data;

	@JsonIgnore
	public String getCode() {
		return code;
	}

	@JsonIgnore
	public void setCode(String code) {
		this.code = code;
	}

	@JsonIgnore
	public String getFlag() {
		return flag;
	}

	@JsonIgnore
	public void setFlag(String flag) {
		this.flag = flag;
	}

	@JsonIgnore
	public String getSession() {
		return session;
	}

	@JsonIgnore
	public void setSession(String session) {
		this.session = session;
	}

	@JsonIgnore
	public String getTimestamp() {
		return timestamp;
	}

	@JsonIgnore
	public void setTimestamp(String timestamp) {
		this.timestamp = timestamp;
	}

	@JsonIgnore
	public String getFrom() {
		return from;
	}

	@JsonIgnore
	public void setFrom(String from) {
		this.from = from;
	}

	@JsonIgnore
	public String getTo() {
		return to;
	}

	@JsonIgnore
	public void setTo(String to) {
		this.to = to;
	}

	@JsonIgnore
	public int getTotal() {
		return total;
	}

	@JsonIgnore
	public void setTotal(int total) {
		this.total = total;
	}

	@JsonIgnore
	public Object getData() {
		return data;
	}

	@JsonIgnore
	public void setData(Object data) {
		this.data = data;
	} 
}
