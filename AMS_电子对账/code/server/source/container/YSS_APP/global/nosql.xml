<nosql>
	<type>mongodb</type>
	<enable>false</enable>
	<servers>
		<server ip="192.168.1.123" port="4444"></server>
		<!--
		<server ip="192.168.1.123" port="5555"></server>
		<server ip="192.168.1.123" port="6666"></server>
		-->
