	<modelVersion>4.0.0</modelVersion>
	<artifactId>com.yss.ams.sec.information</artifactId>
	<name>com.yss.ams.sec.information</name>
	<version>${ams.zxxx.version}</version>

	<dependencies>
		<!-- lib中的依赖包 -->
