			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name = "DATEDIFF" method = "DATEDIFF">
			<parameter-type>java.util.Date</parameter-type>
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name = "DATEDIFF_30_360" method = "DATEDIFF_30_360">
			<parameter-type>java.util.Date</parameter-type>
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name="YSSD_MUL" method="YSSD_MUL">
			<parameter-type>double</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
		<function name="YSSD_DIV" method="YSSD_DIV">
			<parameter-type>double</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
		<function name="YSSD_MULD" method="YSSD_MULD">
			<parameter-type>double</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
		<function name="YSSD_DIVD" method="YSSD_DIVD">
			<parameter-type>double</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
		<function name="YSSD_MULBD" method="YSSD_MULBD">
			<parameter-type>java.math.BigDecimal</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
		<function name="YSSD_DIVBD" method="YSSD_DIVBD">
			<parameter-type>java.math.BigDecimal</parameter-type>
			<parameter-type>double</parameter-type>
		</function>
	</bean>

