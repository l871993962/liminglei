<?xml version="1.0" encoding="UTF-8"?>
<function-configuration>
	
	<!-- 用户函数配置  请在这里定制您自己的函数-->

	<bean class="com.yss.framework.tools.function.IKFunctions">
		<function name="DATE" method="DATE">
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name="ROUND" method="ROUND">
			<parameter-type>double</parameter-type>
			<parameter-type>int</parameter-type>
		</function>
		<function name="STRING" method="STRING">
			<parameter-type>java.lang.String</parameter-type>
		</function>
		<function name="BOOLEAN" method="BOOLEAN">
			<parameter-type>boolean</parameter-type>
		</function>		
		<function name = "DATEYEAR" method = "DATEYEAR">
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name = "DATEMONTH" method = "DATEMONTH">
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name = "DATEDAY" method = "DATEDAY">
			<parameter-type>java.util.Date</parameter-type>
		</function>
		<function name = "LEAPYEARS" method = "LEAPYEARS">
			<parameter-type>java.util.Date</parameter-type>
			<parameter-type>java.util.Date</parameter-type>
		</function>
