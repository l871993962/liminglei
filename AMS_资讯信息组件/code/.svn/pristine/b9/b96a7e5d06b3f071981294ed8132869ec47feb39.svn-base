	public String getN_SUB_MONEY() {
		return n_SUB_MONEY;
	}





	public void setN_SUB_MONEY(String nSUBMONEY) {
		n_SUB_MONEY = nSUBMONEY;
	}





	public String getN_REDEM_MONEY() {
		return n_REDEM_MONEY;
	}





	public void setN_REDEM_MONEY(String nREDEMMONEY) {
		n_REDEM_MONEY = nREDEMMONEY;
	}





	public String getC_MKT_CODE() {
		return c_MKT_CODE;
	}





	public void setC_MKT_CODE(String cMKTCODE) {
		c_MKT_CODE = cMKTCODE;
	}





	public String getC_DC_CODE_REPL() {
		return c_DC_CODE_REPL;
