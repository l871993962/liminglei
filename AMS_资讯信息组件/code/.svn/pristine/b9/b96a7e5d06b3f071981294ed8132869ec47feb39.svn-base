package com.yss.ams.sec.information.support.modules.ac.etfskep.pojo;


import com.yss.framework.api.common.co.BasePojo;


/**
 * ETF股票篮子 pojo类
 * @author gongyue
 * 资讯信息拆分	2017.7.5 STORY #42948 资讯信息管理组件化拆分
 */
public class EtfSkep extends BasePojo {
	/**
	* 业务日期 
	*/
	private String d_TRADE = "";

	/**
	* ETF基金代码（二级代码） 
	*/
	private String c_TRADE_CODE = "";

	/**
	* 证券代码 
	*/
	private String c_SEC_CODE = "";

	/**
	* 证券简称 
	*/
	private String c_SEC_NAME = "";

	/**
	* 证券数量 
	*/
	private String n_AMOUNT = "0";

	/**
	* 替代标志(上交所：表示该成份产品是否允许用现金进行替代。 
	*/
	private String c_REP_MARK = "";

	/**
	* 溢价比例 
	*/
	private String n_RATIO = "0";

	/**
	* 币种代码 
	*/
	private String c_DC_CODE = "";

	/**
	* 申购替代金额 
	*/
	private String n_SUB_MONEY = "0";

	/**
	* 赎回替代金额 
	*/
	private String n_REDEM_MONEY = "0";

	/**
	* 成分股市场 
	*/
	private String c_MKT_CODE = "";

	/**
	* 替代币种 
	*/
	private String c_DC_CODE_REPL = "";


	
	

	public String getD_TRADE() {
		return d_TRADE;
	}





	public void setD_TRADE(String dTRADE) {
		d_TRADE = dTRADE;
	}





	public String getC_TRADE_CODE() {
		return c_TRADE_CODE;
	}





	public void setC_TRADE_CODE(String cTRADECODE) {
		c_TRADE_CODE = cTRADECODE;
	}





	public String getC_SEC_CODE() {
		return c_SEC_CODE;
	}





	public void setC_SEC_CODE(String cSECCODE) {
		c_SEC_CODE = cSECCODE;
	}





	public String getC_SEC_NAME() {
		return c_SEC_NAME;
	}





	public void setC_SEC_NAME(String cSECNAME) {
		c_SEC_NAME = cSECNAME;
	}





	public String getN_AMOUNT() {
		return n_AMOUNT;
	}





	public void setN_AMOUNT(String nAMOUNT) {
		n_AMOUNT = nAMOUNT;
	}





	public String getC_REP_MARK() {
		return c_REP_MARK;
	}





	public void setC_REP_MARK(String cREPMARK) {
		c_REP_MARK = cREPMARK;
	}





	public String getN_RATIO() {
		return n_RATIO;
	}





	public void setN_RATIO(String nRATIO) {
		n_RATIO = nRATIO;
	}





	public String getC_DC_CODE() {
